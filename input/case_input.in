[Case]
 num_elements = 10

[Simulation] 
 print_freq = 10

[wave]
 wave_speed = 1;

[space_solver]
  order = 3;
  upwind_biased = 1  # 1: for 3rd order scheme with upwind biased flux. If '0' then it is a fully upwind scheme
 
[time_solver]
 CFL_no = 0.01

 dt = 1e-5
 initial_time = 0

 final_time = 5.0
 
 maximum_iteration = 1e6

 [./explicit]

  RK_order = 3
